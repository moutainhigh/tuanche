    select
    <include refid="Base_Column_List" />
    from t_enterprise
    where 1 = 1
    <if test="manger != null and manger != ''">
      AND  manger = #{manger,jdbcType=VARCHAR}
    </if>

    <if test="enterpriseName != null and enterpriseName != ''">
